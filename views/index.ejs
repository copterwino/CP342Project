                                <img src=<%=data[i].conPoster%> alt=data[i].conName style="width:100%"
                                    class="w3-hover-opacity">
                                <div class="w3-container w3-white">
                                    <p><b><%= data[i].artistName %></b></p>
                                    <p class="w3-opacity"><%= data[i].conDate.startDate %> to <%= data[i].conDate.endDate %> </p>
                                    <p><%= data[i].conName %></p>
                                    <button class="w3-button w3-black w3-margin-bottom"
                                        onclick="document.getElementById('ticketModal').style.display='block'">Buy
                                        Tickets</button>
                                </div>
                            </div>
                        <% }
                                     
                     }else{ %>
                                 <tr>
                                    <td colspan="3">No user</td>
                                 </tr>
